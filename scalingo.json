{
  "name": "nodejs",
  "stack": "scalingo-18",
  "description": "nodejs-v1.0",
  "logo": "https://scalingo.com/logo.svg",
  "repository": "https://github.com/dsadsadsss/vvvlllll",
  "website": "https://scalingo.com",
  "env": {
    "NEZHA_SERVER": {
      "description": "哪吒服务器地址",
      "value": "1"
    },
    "NEZHA_KEY": {
      "description": "哪吒密钥",
      "value": "1"
    },
    "SUB_NAME": {
      "description": "节点名称",
      "value": "scalingo"
    },
    "SUB_URL": {
      "description": "上传订阅合集地址",
      "value": "https://xxxx.com"
    },
    "PUBLIC_URL": {
      "description": "保持默认即可",
      "generator": "url"
    },
    "ADMIN_URL": {
      "description": "保持默认即可",
      "generator": "url",
      "template": "%URL%/admin"
    }
  },
  "scripts": {
    "first-deploy": "npm install",
    "postdeploy": "npm start"
  }

}
